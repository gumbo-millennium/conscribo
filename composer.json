{
    "name": "gumbo-millennium/conscribo-api",
    "description": "Easy-to-use Conscribo API usage for membership sync",
    "keywords": [
        "gumbo-millennium",
        "laravel",
        "conscribo-api"
    ],
    "homepage": "https://github.com/gumbo-millennium/conscribo-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Roelof Roos",
            "email": "github@roelof.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Gumbo\\Conscribo\\": "src",
            "Gumbo\\Conscribo\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gumbo\\Conscribo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gumbo\\Conscribo\\ConscriboServiceProvider"
            ],
            "aliases": {
                "Conscribo": "Gumbo\\Conscribo\\Facades\\Conscribo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
